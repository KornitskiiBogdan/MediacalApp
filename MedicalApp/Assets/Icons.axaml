	<Design.PreviewWith>
		<StackPanel MinWidth="200" MaxWidth="1960">
			<StackPanel.Resources>
				<ControlTheme x:Key="IconPreviewer" TargetType="tools:IconPreviewer">
					<Setter Property="Template">
						<ControlTemplate>
							<Border BorderThickness="1" BorderBrush="#FF177E89" Padding="3" Margin="3">
								<DockPanel>
									<RadioButton GroupName="1" Theme="{TemplateBinding Key}" DockPanel.Dock="Right"/>
									<TextBlock Text="{TemplateBinding Name}" Margin="0,0,3,0" />
								</DockPanel>
							</Border>
						</ControlTemplate>
					</Setter>
				</ControlTheme>
			</StackPanel.Resources>
			<StackPanel.Styles>
				<Style Selector="HeaderedContentControl">
					<Setter Property="Theme" Value="{StaticResource PreviewGroup}" />
				</Style>
			</StackPanel.Styles>
			<HeaderedContentControl Header="Radio buttons">
				<ItemsControl>
					<ItemsControl.ItemsPanel>
						<ItemsPanelTemplate>
							<WrapPanel />
						</ItemsPanelTemplate>
					</ItemsControl.ItemsPanel>
					<ItemsControl.ItemTemplate>
						<DataTemplate>
							<tools:IconPreviewer x:Name="{Binding .}" Theme="{StaticResource IconPreviewer}" />
						</DataTemplate>
					</ItemsControl.ItemTemplate>
                    <system:String>AnalysisRegular</system:String>
				</ItemsControl>
			</HeaderedContentControl>

		</StackPanel>
	</Design.PreviewWith>
