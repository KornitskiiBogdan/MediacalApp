<Application xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mediacalApp="clr-namespace:MediacalApp"
             x:Class="MediacalApp.App" RequestedThemeVariant="Light"
             >
             <!-- "Default" ThemeVariant follows system theme variant. "Dark" or "Light" are other available options. -->

    <Application.DataTemplates>
		<mediacalApp:ViewLocator/>
    </Application.DataTemplates>

    <Application.Styles>
        <FluentTheme />
        <StyleInclude Source="avares://MediacalApp/Assets/Icons.axaml" />
    </Application.Styles>
</Application>
